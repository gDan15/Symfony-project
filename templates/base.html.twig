            .title-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 10px/1.5 sans-serif; }
            .title-wrapper { background: #9494b8; padding: 2px 6px; }

            .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 10px/1.5 sans-serif; }
            .example-wrapper { background: #D0F0F2; padding: 2px 6px; }

